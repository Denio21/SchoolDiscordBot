{
  "token": "token of the bot that you can get from discord dev portal",
  "prefix": "! - prefix of the bot",
  "guild": "main guild id where bot operates",
  "language": "en",
  "spam": {
    // seconds before sending another command
    "cooldown": "3"
  },
  "roles": {
    // groups of roles
    // groups that can be mention with event command
    "mentionable": {
      "member": "id of the role"
    },
    // groups that can be assign by click on reaction/using command
    "assignable": {
      "name": "id of the role"
    },
    // special roles, must contain role lock and mute
    "special": {
      "rolelock": "id of the role",
      "mute": "id of the role"
    },
    // permission roles for using commands, must contain member and moderator
    "permission": {
      "member": "id of the role",
      "moderator": "id of the role",
      "admin": "if of the role"
    }
  },
  // list of channels, must contains all values
  "channels": {
    "vote": "id of the channel",
    "supplementation": "id of the channel",
    "role": "id of the channel",
    "event": "id of the channel",
    "event-archive": "id of the channel",
    "news": "id of the channel",
    "annoucement": "id of the channel",
    "admin": "id of the channel"
  },
  "modules": {
    // builder
    "builder": {
      "stopWord": "stop"
    },
    "eventannoucement": {
      "checkTime": [
        "17:00",
        "18:00",
        "19:00",
        "19:00",
        "17:00",
        "15:00",
        "15:00"
      ]
    },
    // auto reaction
    "auto-reaction": {
      // react to any attachment in CHANNEL: EMOJI
      "channels-attachments": {
        "channel id": "emoji id"
      },
      // react to text in any channel with emoji
      "test": {
        "<:emojiname:emojiid>": "emoji id",
        "test": "emoji id"
      },
      // react to text with series of emojis
      "series-text": {
        "test": [
          "emoji",
          "üá≠",
          "or emoji id",
          "üá¶",
          "üá∞"
          "üá™",
          "üá©"
        ]
      }
    },
    "weather":{
      // unit for weather command. C and F supported
      "degreeUnit": "F"
    },
    "supplementation": {
      // highlights for supplementation
      "highlights": [
        "3.C",
        "3. roƒçn√≠ky",
        "3. roƒçn√≠k"
      ],
      // Things to replace inside supplementation
      "replace": {
        "what": "to"
      },
      // refresh time of supplementation
      "refresh": 900000
    },
    "mute": {
      "role": "role id",
      // max mute length in minutes
      "max": 10080
    },
    "event": {
      // no. of days of event to move to archive
      "days": 1,
      // refresh time of events
      "refresh": 1800000,
      "placeholder": {
        "name": "n. of days"
      }
    },
    "bakalari": {
      "members": {
        "matej": {
          "domain": "bakalari.api.com",
          "url": "/bakalari.ashx?bk=with-api-key",
          "fullUrl": "https://full.path.to.bakalari.api",
          "group": "1"
        },
        "jakub": {
          "domain": "bakalari.api.com",
          "url": "/bakalari.ashx?bk=with-api-key",
          "fullUrl": "https://full.path.to.bakalari.api",
          "group": "2"
        }
      },
      "ignored": [
        "subject"
      ],
      "separated": [
        "subject"
      ]
    },
    "disabled": [
      [
        "supplementationmodule",
        "bakalarimoadule"
      ]
    ]
  },
  "commands": {
    "disabled": [
      // ["purge"]
    ]
  }
}